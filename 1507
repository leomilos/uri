#include<iostream>
#include<vector>
#include<queue>
#include <cstdlib>
#include <string>

using namespace std;

vector< vector<int> > g; // Lista de AdjacÃªncia


int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){

        string a;
        cin>>a;
        int m;
        cin>>m;
        for(int j=0;j<m;j++){
            string b;
            cin>>b;
            int cont=0;
            for(int k=0;k<a.size();k++){
                if(a[k]==b[cont]){
                    cont++;
                }
            }
            if(cont==b.size()){
                cout<<"Yes"<<endl;
            }else{
                cout<<"No"<<endl;
            }
        }

    }

    system("pause");

    return 0;
}
